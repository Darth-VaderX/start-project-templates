{
    "name": "webpack-basic",
    "version": "1.0.0",
    "description": "app",
    "main": "index.js",
    "scripts": {
        "dev": "npm run format && npm run lint:fix && cross-env NODE_ENV=development webpack --mode development",
        "build": "cross-env NODE_ENV=production webpack --mode production",
        "dev-server": "cross-env NODE_ENV=development webpack serve --mode development --open",
        "start": "webpack serve --open --config ./webpack.config.js --env mode=dev",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint --fix ./src",
        "format": "prettier --write ."
    },
    "browserslist": [
        "last 2 versions",
        "> 0.5%",
        "IE 11"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^7.3.1",
        "@typescript-eslint/parser": "^7.3.1",
        "autoprefixer": "^10.4.18",
        "babel-loader": "^9.1.3",
        "clean-webpack-plugin": "^4.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.10.0",
        "css-minimizer-webpack-plugin": "^6.0.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-webpack-plugin": "^4.1.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.6.0",
        "mini-css-extract-plugin": "^2.8.1",
        "postcss-loader": "^8.1.1",
        "prettier": "^3.2.5",
        "sass": "^1.72.0",
        "sass-loader": "^14.1.1",
        "style-loader": "^3.3.4",
        "terser-webpack-plugin": "^5.3.10",
        "ts-loader": "^9.5.1",
        "typescript": "^5.4.2",
        "typescript-eslint": "^7.3.1",
        "webpack": "^5.90.3",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4"
    },
    "dependencies": {
        "reset-css": "^5.0.2"
    }
}
