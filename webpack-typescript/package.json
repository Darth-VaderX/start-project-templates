  "browserslist": [
    "last 2 versions",
    "> 0.5%",
    "IE 11"
  ],
  "author": "RectalHook",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.3.1",
